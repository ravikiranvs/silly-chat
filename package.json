{
  "name": "test-socketio",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "node dist/app.js",
    "serve": "node server",
    "build:prod": "SET NODE_ENV=production&& webpack -p && babel server -d dist",
    "build:dev": "SET NODE_ENV=development&& webpack --watch",
