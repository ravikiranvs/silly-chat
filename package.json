    "express": "^4.13.3",
    "fs": "^0.0.2",
    "nodemailer": "^6.4.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "rivescript": "^1.13.0",
    "socket.io": "^1.3.6"
  },
  "engines": {
    "node": "6.2.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.11.1",
