    "nock": "^9.0.2",
    "npm-run-all": "^2.3.0",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.10",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-livereload-plugin": "^0.8.2"
